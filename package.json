{
	"name": "imran-khan",
	"version": "1.0.0",
	"author": "Imran Khan",
	"private": true,
	"scripts": {
		"ng": "ng",
		"start": "ng serve --port 1500",
		"build": "ng build",
		"serve.app.next": "ng serve -c next --port 2000",
		"build.app.next": "ng build -c next",
		"serve.app.stag": "ng serve -c stag --port 3000",
		"build.app.stag": "ng build -c stag",
		"serve.app.prod": "ng serve -c prod --port 4000",
		"build.app.prod": "ng build -c prod",
		"serve.app.prod-en": "ng serve -c prod-en --port 4001",
		"build.app.prod-en": "ng build -c prod-en",
		"serve.app.prod-de": "ng serve -c prod-de --port 4002",
		"build.app.prod-de": "ng build -c prod-de",
		"lint:ts": "ng lint",
		"lint:scss": "stylelint \"src/**/*.scss\" --syntax scss",
		"translate": "ng xi18n --output-path locale --out-file translation-temp.xlf",
		"translate:ts": "ngx-extractor --input=src/**/*.ts --format=xlf --out-file=src/locale/translation-temp.xlf"
	},
	"dependencies": {
		"@angular/animations": "^9.0.6",
		"@angular/common": "^9.0.6",
		"@angular/compiler": "^9.0.6",
		"@angular/core": "^9.0.6",
		"@angular/forms": "^9.0.6",
		"@angular/localize": "^9.0.6",
		"@angular/platform-browser": "^9.0.6",
		"@angular/platform-browser-dynamic": "^9.0.6",
		"@angular/router": "^9.0.6",
		"@fortawesome/angular-fontawesome": "^0.6.0",
		"@fortawesome/fontawesome-svg-core": "^1.2.27",
		"@fortawesome/free-brands-svg-icons": "^5.12.1",
		"@fortawesome/free-solid-svg-icons": "^5.12.1",
		"@imransilvake/scss-framework": "^2.2.1",
		"hammerjs": "^2.0.8",
		"lg-autoplay.js": "^1.0.0",
		"lg-fullscreen.js": "^1.1.0",
		"lg-zoom.js": "^1.0.1",
		"lightgallery.js": "^1.1.3",
		"moment": "^2.24.0",
		"rxjs": "^6.5.4",
		"tslib": "^1.11.1",
		"zone.js": "~0.10.2"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.900.6",
		"@angular/cli": "~9.0.6",
		"@angular/compiler-cli": "^9.0.6",
		"@angular/language-service": "^9.0.6",
		"@types/node": "~13.9.1",
		"codelyzer": "^5.2.1",
		"stylelint": "^13.2.1",
		"stylelint-order": "^4.0.0",
		"ts-node": "~8.6.2",
		"tslint": "~6.1.0",
		"typescript": "~3.7.5"
	}
