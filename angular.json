{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"ham": {
			"root": "",
			"sourceRoot": "src",
			"projectType": "application",
			"prefix": "app",
			"schematics": {
				"@schematics/angular:component": {
					"styleext": "scss",
					"spec": false
				},
				"@schematics/angular:module": {
					"spec": false
				},
				"@schematics/angular:service": {
					"spec": false
				},
				"@schematics/angular:pipe": {
					"spec": false
				},
				"@schematics/angular:directive": {
					"spec": false
				}
			},
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/ikhan",
						"index": "src/index.html",
						"main": "src/main.ts",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "src/tsconfig.app.json",
						"aot": true,
						"assets": [
							"src/favicon.ico",
							"src/assets/svg",
							"src/assets/images"
						],
						"styles": [
							"src/styles.scss"
						],
						"stylePreprocessorOptions": {
							"includePaths": [
								"src/assets/scss/"
							]
						},
						"scripts": [
							"node_modules/lightgallery.js/dist/js/lightgallery.min.js",
							"node_modules/lg-fullscreen.js/dist/lg-fullscreen.min.js",
							"node_modules/lg-zoom.js/dist/lg-zoom.min.js",
							"node_modules/lg-autoplay.js/dist/lg-autoplay.min.js"
						]
					},
					"configurations": {
						"stag": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.stag.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true
						},
						"next": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.next.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true
						},
						"prod": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true
						},
						"prod-en": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"outputPath": "dist/ik-en/",
							"baseHref": "/en/",
							"i18nFile": "src/locale/translation.en-US.xlf",
							"i18nFormat": "xlf",
							"i18nLocale": "en-US",
							"i18nMissingTranslation": "error"
						},
						"prod-de": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"outputPath": "dist/ik-de/",
							"baseHref": "/de/",
							"i18nFile": "src/locale/translation.de.xlf",
							"i18nFormat": "xlf",
							"i18nLocale": "de",
							"i18nMissingTranslation": "error"
						}
					}
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "ham:build"
					},
					"configurations": {
						"stag": {
							"browserTarget": "ham:build:stag"
						},
						"next": {
							"browserTarget": "ham:build:next"
						},
						"prod": {
							"browserTarget": "ham:build:prod"
						},
						"prod-en": {
							"browserTarget": "ham:build:prod-en"
						},
						"prod-de": {
							"browserTarget": "ham:build:prod-de"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "ham:build"
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"src/tsconfig.app.json"
						],
						"exclude": [
							"**/node_modules/**"
						]
					}
				}
			}
		}
	},
	"defaultProject": "ham"
}
