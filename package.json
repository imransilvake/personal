{
	"name": "imran-khan",
	"version": "1.1.0",
	"author": "Imran Khan",
	"private": true,
	"scripts": {
		"ng": "ng",
		"start": "ng serve --port 1500",
		"build": "ng build",
		"serve.app.next": "ng serve -c next --port 2000",
		"build.app.next": "ng build -c next",
		"serve.app.prod": "ng serve -c prod --port 3000",
		"build.app.prod": "ng build -c prod",
		"lint:ts": "ng lint",
		"lint:scss": "stylelint \"src/**/*.scss\" --syntax scss"
	},
	"dependencies": {
		"@angular/animations": "^11.0.2",
		"@angular/common": "^11.0.2",
		"@angular/compiler": "^11.0.2",
		"@angular/core": "^11.0.2",
		"@angular/fire": "^6.1.2",
		"@angular/forms": "^11.0.2",
		"@angular/platform-browser": "^11.0.2",
		"@angular/platform-browser-dynamic": "^11.0.2",
		"@angular/router": "^11.0.2",
		"@fortawesome/angular-fontawesome": "^0.8.0",
		"@fortawesome/fontawesome-svg-core": "^1.2.32",
		"@fortawesome/free-brands-svg-icons": "^5.15.1",
		"@fortawesome/free-regular-svg-icons": "^5.15.1",
		"@fortawesome/free-solid-svg-icons": "^5.15.1",
		"@imransilvake/scss-framework": "^2.3.2",
		"@ngx-translate/core": "^13.0.0",
		"@ngx-translate/http-loader": "^6.0.0",
		"firebase": "^8.1.1",
		"hammerjs": "^2.0.8",
		"moment": "^2.29.1",
		"rxjs": "^6.6.3",
		"tslib": "^2.0.3",
		"zone.js": "~0.11.3"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.1100.2",
		"@angular/cli": "~11.0.2",
		"@angular/compiler-cli": "^11.0.2",
		"@angular/language-service": "^11.0.2",
		"@semantic-release/changelog": "^5.0.1",
		"@semantic-release/git": "^9.0.0",
		"@types/node": "~14.14.10",
		"codelyzer": "^6.0.1",
		"stylelint": "^13.8.0",
		"stylelint-order": "^4.1.0",
		"ts-node": "~9.0.0",
		"tslint": "~6.1.3",
		"typescript": "~4.1.2",
		"eslint": "^7.6.0",
		"eslint-plugin-import": "2.22.1",
		"eslint-plugin-jsdoc": "30.7.6",
		"eslint-plugin-prefer-arrow": "1.2.2",
		"@angular-eslint/builder": "0.8.0-beta.2",
		"@angular-eslint/eslint-plugin": "0.8.0-beta.2",
		"@angular-eslint/eslint-plugin-template": "0.8.0-beta.2",
		"@angular-eslint/schematics": "^0.8.0-beta.2",
		"@angular-eslint/template-parser": "0.8.0-beta.2",
		"@typescript-eslint/eslint-plugin": "4.3.0",
		"@typescript-eslint/parser": "4.3.0"
	},
	"plugins": [
		"@semantic-release/commit-analyzer",
		"@semantic-release/release-notes-generator",
		"@semantic-release/changelog",
		"@semantic-release/npm",
		"@semantic-release/git",
		"@semantic-release/github"
	],
	"release": {
		"prepare": [
			"@semantic-release/changelog",
			"@semantic-release/npm",
			{
				"path": "@semantic-release/git",
				"assets": [
					"package.json",
					"CHANGELOG.md"
				],
				"message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
			}
		]
	}
}